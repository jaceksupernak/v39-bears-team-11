{
  "extends": ["airbnb"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": 0,
    "semi": ["error", "never"],
    "react/prop-types": "off",
    "no-console": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/jsx-props-no-spreading": "off",
    "import/no-named-as-default": 0,
    "import/prefer-default-export": "off"
  }
}
